wmic /node:COMPUTERNAME useraccount get name,sid
wmic /node:REMOTECOMPUTERNAME path win32_userprofile  // list of profiles
wmic /node:REMOTECOMPUTERNAME path win32_userprofile where sid="S-******" delete
wmic /node:REMOTECOMPUTERNAME path win32_userprofile where localpath="C:\\Users\\profileName" Delete
PowerShell

 Get-WmiObject -Class Win32_UserProfile -ComputerName computerName -Filter "LocalPath='C:\Users\profileName'"
Get-WmiObject -ComputerName NWKPBAM7563273 -Class win32_userProfile | Where-Object {$_.LocalPath -like "C:\Users\ProfileName"} | ForEach-Object {$_.LocalPath}
or $_.Delete()
or |Remove-WmiObject


(Get-WmiObject Win32_Processor).LoadPercentage
Get-WmiObject -class win32_processor -computername. | select-object -property [a-z]*

wmic /namespace:\\root\wmi PATH MSAcpi_ThermalZoneTemperature get currenttemperature

get-wmiobject -class msacpi_thermalzonetemperature -namespace "root/wmi"


se the following commands at a command prompt. Type the following to enable WMI traffic through the firewall.

netsh advfirewall firewall set rule group="windows management instrumentation (wmi)" new enable=yes

Type the following command to disable WMI traffic through the firewall.

netsh advfirewall firewall set rule group="windows management instrumentation (wmi)" new enable=no

Get-WmiObject -Class Win32_NetworkAdapterConfiguration -ComputerName RemoteComputerName | Select-Object Description, IPAddress, DNSHostName

Get-NetFirewallRule
